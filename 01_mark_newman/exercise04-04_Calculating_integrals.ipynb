   "source": [
    "%time\n",
    "N = 10000\n",
    "h = 2/N\n",
    "def yk(xk):\n",
    "    return sqrt(1 - xk**2)\n",
    "\n",
    "square = 0\n",
    "\n",
    "for k in range(N):\n",
    "    square += yk(-1 + h*k)*h\n",
    "print(square)"
   ]
